        <script>
            var app = angular.module("myapp", ['ngRoute'])
                .constant("bookingurlurl", "http://dvents.azurewebsites.net/odata/BookingDetails")
            .config(function ($locationProvider, $routeProvider) {
                $locationProvider.hashPrefix('');
                //$locationProvider.html5Mode({
                //    enabled: true,
                //    requireBase: false
                //});
                $routeProvider.when('/About', {
                    templateUrl: '/app/Views/About.html',
                }).when('/Contact', {
                    templateUrl: '/app/Views/Contact.html',
                }).when('/Catagoryview', {
                    templateUrl: '/app/Views/Catagoryview.html'
                }).when('/productview', {
                    templateUrl: '/app/Views/productview.html'
                }).when('/bookingView', {
                    templateUrl: '/app/Views/bookingView.html'
                }).when('/CheckoutSummery', {
                    templateUrl: '/app/Views/CheckoutSummery.html'
                }).when('/eventview', {
                    templateUrl: '/app/Views/eventview.html'
                }).when('/p', {
                    templateUrl: '/app/Views/PlaceOrder.html'
                }).when('/index', {
                    templateUrl: '/index.html'
                }).when('/services', {
                    templateUrl: '/services.html'
                }).when('/about', {
                    templateUrl: '/about.html'
                }).when('/contact', {
                    templateUrl: '/contact.html'
                }).when('/placeOrder', {
                    templateUrl:'/app/Views/PlaceOrder.html'
                }).when('/thanks', {
                    templateUrl: '/app/Views/Thanks.html'
                })
                    .otherwise({
                    templateUrl: '/home.html'
                })
            }).controller("mainctrl", function ($scope) {
                $scope.rate = 83.0;
                $scope.model.FoodCost = null;

            })
        </script>
